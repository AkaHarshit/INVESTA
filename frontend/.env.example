# Frontend environment variables (copy to .env and fill in)
# For Vercel: set these in Project → Settings → Environment Variables

# Backend API base URL (no trailing slash)
# Local: http://localhost:3002
# Production: https://your-backend.onrender.com
REACT_APP_API_URL=http://localhost:3002

# Dashboard app URL (where "Open Dashboard" redirects)
# Local: http://localhost:3001
# Production: https://your-dashboard.vercel.app
REACT_APP_DASHBOARD_URL=http://localhost:3001
